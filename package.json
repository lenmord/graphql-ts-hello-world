{
  "name": "graphql-ts-hello-world",
  "version": "1.0.0",
  "description": "A hello-world api project built with typescript, apollo, and graphql",
  "main": "index.js",
  "scripts": {
    "test": "npm run lint && jest --testRegex \"(/__tests__/.*)\\.test.ts?$\" --coverage",
    "test:smoke": "jest --testRegex \"(/__smoke__/.*)\\.test.ts$\"",
    "start:build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "lint": "eslint . --ext .ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AppDirect/graphql-ts-hello-world.git"
  },
  "jest": {
    "preset": "ts-jest",
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.ts?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AppDirect/graphql-ts-hello-world/issues"
  },
  "homepage": "https://github.com/AppDirect/graphql-ts-hello-world#readme",
  "dependencies": {
    "@godaddy/terminus": "^4.6.0",
    "@types/compression": "^1.7.0",
    "apollo-datasource-rest": "^0.10.0",
    "apollo-server": "^2.21.0",
    "apollo-server-caching": "^0.5.3",
    "apollo-server-express": "^2.21.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "express-unless": "^0.5.0",
    "graphql": "^15.5.0",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^1.12.2",
    "morgan": "^1.10.0",
    "node-fetch": "^2.6.1",
    "nodemon": "^2.0.7",
    "opossum": "^5.1.3",
    "supertest": "^6.1.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.5",
    "winston": "^3.3.3",
    "zipkin": "^0.22.0",
    "zipkin-instrumentation-express": "^0.22.0",
    "zipkin-transport-http": "^0.22.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.28",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "eslint": "^7.20.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "ts-jest": "^26.5.1"
  }
}
